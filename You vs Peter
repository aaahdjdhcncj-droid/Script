local KTab = Window:CreateTab("ü§£ Misc ü§£", nil) -- Title, Image
local Section = KTab:CreateSection("Misc")

local Toggle = KTab:CreateToggle({
    Name = "Funny Auto Farm",
    CurrentValue = false,
    Flag = "ToggleGirarPeter",
    Callback = function(Value)
        _G.GirandoPeter = Value
        
        if Value then
            -- Inicia o loop de rota√ß√£o orbital
            spawn(function()
                local Players = game:GetService("Players")
                local LocalPlayer = Players.LocalPlayer
                local RunService = game:GetService("RunService")
                
                local angulo = 0
                local raio = 12 -- 25 Studs de dist√¢ncia
                local velocidade = 3 -- Velocidade de rota√ß√£o (graus por frame)
                
                local conexao
                conexao = RunService.Heartbeat:Connect(function()
                    if not _G.GirandoPeter then
                        conexao:Disconnect()
                        return
                    end
                    
                    -- Verifica se est√° no Time Brian
                    if LocalPlayer.Team and LocalPlayer.Team.Name == "Brian" then
                        -- Procura por um jogador do Team Peter
                        local targetPlayer = nil
                        
                        for _, player in pairs(Players:GetPlayers()) do
                            if player.Team and player.Team.Name == "Peter" and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                                targetPlayer = player
                                break
                            end
                        end
                        
                        if targetPlayer and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local targetRoot = targetPlayer.Character.HumanoidRootPart
                            local playerRoot = LocalPlayer.Character.HumanoidRootPart
                            
                            -- Incrementa o √¢ngulo para orbitar
                            angulo = angulo + velocidade
                            if angulo >= 360 then
                                angulo = 0
                            end
                            
                            -- Converte √¢ngulo para radianos
                            local anguloRad = math.rad(angulo)
                            
                            -- Calcula a nova posi√ß√£o orbital
                            local offsetX = raio * math.cos(anguloRad)
                            local offsetZ = raio * math.sin(anguloRad)
                            
                            local novaPosicao = targetRoot.Position + Vector3.new(offsetX, 0, offsetZ)
                            
                            -- Move o jogador e faz olhar para o alvo
                            playerRoot.CFrame = CFrame.new(novaPosicao, targetRoot.Position)
                        end
                    end
                end)
            end)
        end
    end,
})

