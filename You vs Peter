local Button = MTab:CreateButton({
   Name = "Noclip",
   Callback = function()
   -- Script de Noclip (atravessar paredes mas n√£o o ch√£o)
-- Para Roblox - Ativa automaticamente

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

local runService = game:GetService("RunService")

print("Noclip ATIVADO - Voc√™ pode atravessar paredes!")

-- Loop principal do noclip (sempre ativo)
runService.Stepped:Connect(function()
    if character then
        -- Percorre todas as partes do personagem
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                -- Mant√©m colis√£o com o ch√£o, mas remove com paredes
                -- Verifica se a parte est√° acima do ch√£o
                local ray = Ray.new(part.Position, Vector3.new(0, -5, 0))
                local hit, position = workspace:FindPartOnRay(ray, character)
                
                -- Se houver ch√£o perto, mant√©m a colis√£o
                if hit and (part.Position.Y - position.Y) < 4 then
                    part.CanCollide = false
                    -- Reativa colis√£o apenas para HumanoidRootPart quando pr√≥ximo do ch√£o
                    if part == humanoidRootPart then
                        part.CanCollide = false
                    end
                else
                    -- Remove colis√£o com paredes
                    part.CanCollide = false
                end
            end
        end
    end
end)

print("Script de Noclip carregado e funcionando!")
   end,
})

local Button = MTab:CreateButton({
   Name = "TpTool",
   Callback = function()
   -- Script de Teleport Tool
-- Cria uma tool que teleporta voc√™ onde clicar

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local backpack = player:WaitForChild("Backpack")

-- Criar a tool
local tpTool = Instance.new("Tool")
tpTool.Name = "TpTool"
tpTool.RequiresHandle = false
tpTool.CanBeDropped = false
tpTool.Parent = backpack

print("Tool de teleporte criada: tptool")

-- Fun√ß√£o de teleporte ao clicar
tpTool.Activated:Connect(function()
    local mouse = player:GetMouse()
    local targetPosition = mouse.Hit.Position
    
    if targetPosition then
        -- Teleporta o jogador
        humanoidRootPart.CFrame = CFrame.new(targetPosition + Vector3.new(0, 3, 0))
        print("Teleportado para: " .. tostring(targetPosition))
    end
end)

print("Tool de teleporte ativada! Equipe e clique onde quiser ir!")
   end,
})

local GTab = Window:CreateTab("üéÆ Game üéÆ", nil) -- Title, Image
local Section = GTab:CreateSection("Game")

local Button = GTab:CreateButton({
   Name = "Peter Auto Win",
   Callback = function()
   -- Script de Teleporte Autom√°tico por Time
-- Coloque este script em StarterPlayer > StarterPlayerScripts

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Fun√ß√£o para verificar se o jogador local est√° no Team Peter
local function isPlayerInTeamPeter()
    if LocalPlayer.Team and LocalPlayer.Team.Name == "Peter" then
        return true
    end
    return false
end

-- Fun√ß√£o para teleportar um jogador at√© voc√™
local function teleportPlayerToMe(targetPlayer)
    if not isPlayerInTeamPeter() then
        warn("Voc√™ precisa estar no Team Peter para teleportar!")
        return
    end
    
    local character = LocalPlayer.Character
    local targetCharacter = targetPlayer.Character
    
    if character and targetCharacter then
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        local targetRootPart = targetCharacter:FindFirstChild("HumanoidRootPart")
        
        if humanoidRootPart and targetRootPart then
            -- Teleporta o jogador do Team Brian exatamente para sua posi√ß√£o
            targetRootPart.CFrame = humanoidRootPart.CFrame
            print("Teleportado " .. targetPlayer.Name .. " para voc√™")
        end
    end
end

-- Fun√ß√£o principal para processar players do Team Brian
local function processTeamBrian()
    if not isPlayerInTeamPeter() then
        return
    end
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Team and player.Team.Name == "Brian" then
            teleportPlayerToMe(player)
            wait(0.3) -- Pequeno delay entre teleportes
        end
    end
end

-- Monitorar quando novos jogadores entram no Team Brian
Players.PlayerAdded:Connect(function(player)
    player:GetPropertyChangedSignal("Team"):Connect(function()
        if player.Team and player.Team.Name == "Brian" and isPlayerInTeamPeter() then
            wait(1) -- Espera o personagem spawnar
            teleportPlayerToMe(player)
        end
    end)
end)

-- Monitorar mudan√ßas de time dos jogadores j√° existentes
for _, player in pairs(Players:GetPlayers()) do
    if player ~= LocalPlayer then
        player:GetPropertyChangedSignal("Team"):Connect(function()
            if player.Team and player.Team.Name == "Brian" and isPlayerInTeamPeter() then
                wait(1)
                teleportPlayerToMe(player)
            end
        end)
    end
end

-- Monitorar quando voc√™ muda de time
LocalPlayer:GetPropertyChangedSignal("Team"):Connect(function()
    if isPlayerInTeamPeter() then
        print("Voc√™ entrou no Team Peter! Teleporte ativado.")
        wait(1)
        processTeamBrian()
    else
        print("Voc√™ n√£o est√° no Team Peter. Teleporte desativado.")
    end
end)

-- Executar imediatamente ao carregar
wait(2) -- Espera o jogo carregar
if isPlayerInTeamPeter() then
    print("Script ativado! Voc√™ est√° no Team Peter.")
    processTeamBrian()
else
    print("Aguardando voc√™ entrar no Team Peter...")
end

-- Loop cont√≠nuo para verificar (a cada 3 segundos)
while true do
    wait(3)
    if isPlayerInTeamPeter() then
        processTeamBrian()
    end
            end
   end,
})

local Button = Tab:CreateButton({
   Name = "Brian Auto Win",
   Callback = function()
   -- Script de Teleporte para Part Personalizada
-- S√≥ funciona quando voc√™ estiver no Team Brian

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

-- Configura√ß√µes
local TEAM_BRIAN_NAME = "Brian" -- Nome do time Brian
local PART_POSITION = Vector3.new(276, 115, 36) -- Posi√ß√£o da part
local PART_SIZE = Vector3.new(6, 1, 6) -- Tamanho m√©dio/pequeno
local DECAL_ID = "rbxassetid://15886830797" -- ID da decal

-- Aguarda o personagem carregar completamente
local function waitForCharacter()
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 10)
    return character, humanoidRootPart
end

-- Fun√ß√£o para verificar se o jogador local est√° no Team Brian
local function isOnTeamBrian()
    if LocalPlayer.Team and LocalPlayer.Team.Name == TEAM_BRIAN_NAME then
        return true
    end
    return false
end

-- Fun√ß√£o para criar a Part com Decal
local function createPlatform()
    print("Tentando criar plataforma...")
    
    -- Verifica se voc√™ est√° no Team Brian
    if not isOnTeamBrian() then
        warn("Voc√™ precisa estar no Team Brian para usar este script!")
        warn("Seu time atual:", LocalPlayer.Team and LocalPlayer.Team.Name or "Nenhum")
        return nil
    end
    
    print("Voc√™ est√° no Team Brian! Criando plataforma...")
    
    -- Cria a Part
    local platform = Instance.new("Part")
    platform.Name = "BrianPlatform"
    platform.Size = PART_SIZE
    platform.Anchored = true
    platform.CanCollide = true
    platform.Material = Enum.Material.SmoothPlastic
    platform.BrickColor = BrickColor.new("Bright blue")
    platform.TopSurface = Enum.SurfaceType.Studs -- Studs no topo
    platform.BottomSurface = Enum.SurfaceType.Smooth
    platform.Transparency = 0
    
    -- Cria a Decal no topo da Part
    local decal = Instance.new("Decal")
    decal.Name = "TopDecal"
    decal.Face = Enum.NormalId.Top -- Decal no topo
    decal.Texture = DECAL_ID
    decal.Transparency = 0
    decal.Parent = platform
    
    -- Adiciona a Part ao Workspace primeiro
    platform.Parent = workspace
    
    -- Depois define a posi√ß√£o (mais confi√°vel)
    platform.CFrame = CFrame.new(PART_POSITION)
    
    print("Plataforma criada com sucesso na posi√ß√£o:", PART_POSITION)
    print("Plataforma Parent:", platform.Parent)
    return platform
end

-- Fun√ß√£o para teleportar para cima da Part
local function teleportToPlatform(humanoidRootPart)
    if humanoidRootPart then
        -- Teleporta para 3 studs acima da posi√ß√£o da plataforma
        local teleportPosition = PART_POSITION + Vector3.new(0, PART_SIZE.Y/2 + 3, 0)
        humanoidRootPart.CFrame = CFrame.new(teleportPosition)
        print("Teleportado para a plataforma na posi√ß√£o:", teleportPosition)
        return true
    else
        warn("HumanoidRootPart n√£o encontrado!")
    end
    return false
end

-- Fun√ß√£o principal
local function executeTeleport()
    print("=== Iniciando script ===")
    
    -- Aguarda o personagem
    local character, humanoidRootPart = waitForCharacter()
    
    if not humanoidRootPart then
        warn("N√£o foi poss√≠vel encontrar o HumanoidRootPart!")
        return
    end
    
    -- Verifica se voc√™ est√° no Team Brian
    if not isOnTeamBrian() then
        warn("Voc√™ precisa estar no Team Brian para usar este script!")
        warn("Seu time atual:", LocalPlayer.Team and LocalPlayer.Team.Name or "Nenhum")
        return
    end
    
    print("Verificando plataforma existente...")
    
    -- Verifica se j√° existe uma plataforma
    local existingPlatform = workspace:FindFirstChild("BrianPlatform")
    
    if existingPlatform then
        -- Se j√° existe, apenas teleporta para a posi√ß√£o
        print("Plataforma j√° existe, teleportando...")
        teleportToPlatform(humanoidRootPart)
    else
        -- Cria nova plataforma
        print("Plataforma n√£o existe, criando...")
        local platform = createPlatform()
        
        if platform then
            wait(0.2) -- Delay para garantir que a part foi criada
            teleportToPlatform(humanoidRootPart)
        else
            warn("Falha ao criar plataforma!")
        end
    end
end

-- Atualiza quando o personagem respawna
LocalPlayer.CharacterAdded:Connect(function(newCharacter)
    wait(1) -- Aguarda o personagem carregar
    executeTeleport()
end)

-- Aguarda um pouco antes de executar (para garantir que tudo carregou)
wait(1)
executeTeleport()
   end,
})

