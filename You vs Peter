local Button = MTab:CreateButton({
   Name = "Noclip",
   Callback = function()
   -- Script de Noclip (atravessar paredes mas n√£o o ch√£o)
-- Para Roblox - Ativa automaticamente

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

local runService = game:GetService("RunService")

print("Noclip ATIVADO - Voc√™ pode atravessar paredes!")

-- Loop principal do noclip (sempre ativo)
runService.Stepped:Connect(function()
    if character then
        -- Percorre todas as partes do personagem
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                -- Mant√©m colis√£o com o ch√£o, mas remove com paredes
                -- Verifica se a parte est√° acima do ch√£o
                local ray = Ray.new(part.Position, Vector3.new(0, -5, 0))
                local hit, position = workspace:FindPartOnRay(ray, character)
                
                -- Se houver ch√£o perto, mant√©m a colis√£o
                if hit and (part.Position.Y - position.Y) < 4 then
                    part.CanCollide = false
                    -- Reativa colis√£o apenas para HumanoidRootPart quando pr√≥ximo do ch√£o
                    if part == humanoidRootPart then
                        part.CanCollide = false
                    end
                else
                    -- Remove colis√£o com paredes
                    part.CanCollide = false
                end
            end
        end
    end
end)

print("Script de Noclip carregado e funcionando!")
   end,
})

local Button = MTab:CreateButton({
   Name = "TpTool",
   Callback = function()
   -- Script de Teleport Tool
-- Cria uma tool que teleporta voc√™ onde clicar

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local backpack = player:WaitForChild("Backpack")

-- Criar a tool
local tpTool = Instance.new("Tool")
tpTool.Name = "tptool"
tpTool.RequiresHandle = false
tpTool.CanBeDropped = false
tpTool.Parent = backpack

print("Tool de teleporte criada: tptool")

-- Fun√ß√£o de teleporte ao clicar
tpTool.Activated:Connect(function()
    local mouse = player:GetMouse()
    local targetPosition = mouse.Hit.Position
    
    if targetPosition then
        -- Teleporta o jogador
        humanoidRootPart.CFrame = CFrame.new(targetPosition + Vector3.new(0, 3, 0))
        print("Teleportado para: " .. tostring(targetPosition))
    end
end)

print("Tool de teleporte ativada! Equipe e clique onde quiser ir!")
   end,
})

local GTab = Window:CreateTab("üéÆ Game üéÆ", nil) -- Title, Image
local Section = GTab:CreateSection("Game")

local Button = GTab:CreateButton({
   Name = "Peter Auto Win",
   Callback = function()
  -- Script de Teleporte para Players do Team Brian
-- S√≥ funciona quando voc√™ estiver no Team Peter

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- Configura√ß√µes
local TEAM_BRIAN_NAME = "Brian" -- Nome do time Brian
local TEAM_PETER_NAME = "Peter" -- Nome do time Peter
local TELEPORT_DELAY = 0.5 -- Delay entre teleportes em segundos

-- Fun√ß√£o para verificar se o jogador local est√° no Team Peter
local function isOnTeamPeter()
    if LocalPlayer.Team and LocalPlayer.Team.Name == TEAM_PETER_NAME then
        return true
    end
    return false
end

-- Fun√ß√£o para teleportar para um jogador espec√≠fico
local function teleportToPlayer(targetPlayer)
    if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetHRP = targetPlayer.Character.HumanoidRootPart
        HumanoidRootPart.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 3)
        print("Teleportado para:", targetPlayer.Name)
        return true
    end
    return false
end

-- Fun√ß√£o para verificar se um jogador est√° no Team Brian
local function isOnTeamBrian(player)
    if player.Team and player.Team.Name == TEAM_BRIAN_NAME then
        return true
    end
    return false
end

-- Fun√ß√£o principal de teleporte
local function teleportToAllBrianPlayers()
    -- Verifica se voc√™ est√° no Team Peter
    if not isOnTeamPeter() then
        warn("Voc√™ precisa estar no Team Peter para usar este script!")
        return
    end
    
    print("Iniciando teleporte para players do Team Brian...")
    local teleportCount = 0
    
    -- Itera por todos os jogadores
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and isOnTeamBrian(player) then
            if teleportToPlayer(player) then
                teleportCount = teleportCount + 1
                wait(TELEPORT_DELAY)
            end
        end
    end
    
    print("Teleporte conclu√≠do! Total de players visitados:", teleportCount)
end
            
-- Atualiza a refer√™ncia do personagem quando respawnar
LocalPlayer.CharacterAdded:Connect(function(newCharacter)
    Character = newCharacter
    HumanoidRootPart = newCharacter:WaitForChild("HumanoidRootPart")
end)

-- Executa o teleporte
teleportToAllBrianPlayers()
   end,
})

local Button = Tab:CreateButton({
   Name = "Brian Auto Win",
   Callback = function()
   -- Script de Teleporte para Part Personalizada
-- S√≥ funciona quando voc√™ estiver no Team Brian

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- Configura√ß√µes
local TEAM_BRIAN_NAME = "Brian" -- Nome do time Brian
local PART_POSITION = Vector3.new(276, 115, 36) -- Posi√ß√£o da part
local PART_SIZE = Vector3.new(6, 1, 6) -- Tamanho m√©dio/pequeno
local DECAL_ID = "rbxassetid://15886830797" -- ID da decal

-- Fun√ß√£o para verificar se o jogador local est√° no Team Brian
local function isOnTeamBrian()
    if LocalPlayer.Team and LocalPlayer.Team.Name == TEAM_BRIAN_NAME then
        return true
    end
    return false
end

-- Fun√ß√£o para criar a Part com Decal
local function createPlatform()
    -- Verifica se voc√™ est√° no Team Brian
    if not isOnTeamBrian() then
        warn("Voc√™ precisa estar no Team Brian para usar este script!")
        return nil
    end
    
    -- Cria a Part
    local platform = Instance.new("Part")
    platform.Name = "BrianPlatform"
    platform.Size = PART_SIZE
    platform.Position = PART_POSITION
    platform.Anchored = true
    platform.CanCollide = true
    platform.Material = Enum.Material.SmoothPlastic
    platform.BrickColor = BrickColor.new("Bright blue")
    platform.TopSurface = Enum.SurfaceType.Studs -- Studs no topo
    platform.BottomSurface = Enum.SurfaceType.Smooth
    
    -- Cria a Decal no topo da Part
    local decal = Instance.new("Decal")
    decal.Name = "TopDecal"
    decal.Face = Enum.NormalId.Top -- Decal no topo
    decal.Texture = DECAL_ID
    decal.Parent = platform
    
    -- Adiciona a Part ao Workspace
    platform.Parent = workspace
    
    print("Plataforma criada na posi√ß√£o:", PART_POSITION)
    return platform
end

-- Fun√ß√£o para teleportar para cima da Part
local function teleportToPlatform()
    if HumanoidRootPart then
        -- Teleporta para 3 studs acima da posi√ß√£o da plataforma
        local teleportPosition = PART_POSITION + Vector3.new(0, PART_SIZE.Y/2 + 3, 0)
        HumanoidRootPart.CFrame = CFrame.new(teleportPosition)
        print("Teleportado para a plataforma!")
        return true
    end
    return false
end

-- Fun√ß√£o principal
local function executeTeleport()
    -- Verifica se voc√™ est√° no Team Brian
    if not isOnTeamBrian() then
        warn("Voc√™ precisa estar no Team Brian para usar este script!")
        return
    end
    
    -- Verifica se j√° existe uma plataforma
    local existingPlatform = workspace:FindFirstChild("BrianPlatform")
    
    if existingPlatform then
        -- Se j√° existe, apenas teleporta para a posi√ß√£o
        print("Plataforma j√° existe, teleportando...")
        teleportToPlatform()
    else
        -- Cria nova plataforma
        local platform = createPlatform()
        
        if platform then
            wait(0.1) -- Pequeno delay para garantir que a part foi criada
            teleportToPlatform()
        end
    end
end

-- Atualiza a refer√™ncia do personagem quando respawnar
LocalPlayer.CharacterAdded:Connect(function(newCharacter)
    Character = newCharacter
    HumanoidRootPart = newCharacter:WaitForChild("HumanoidRootPart")
end)

-- Executa automaticamente
executeTeleport()
   end,
})

